{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "shell:allow-open",
    "fs:default",
    "fs:allow-appcache-read-recursive",
    "fs:allow-app-read-recursive",
    "sql:default",
    "sql:allow-close",
    "sql:allow-execute",
    "sql:allow-load",
    "sql:allow-select"
  ]
}
